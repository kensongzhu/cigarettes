{
  "root": true,
  "plugins": [
    "prettier"

  ],
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "no-unused-vars": "warn",
    "quotes": [
      "error",
      "single"
    ]
  }
}
